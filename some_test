import http.client

conn = http.client.HTTPSConnection("10.11.101.42", 81)

headersList = {
 "Accept": "*/*",
 "User-Agent": "Thunder Client (https://www.thunderclient.com)" 
}

payload = ""

conn.request("GET", "/rc_config/9385807aba71/v2/list/tv/", payload, headersList)
response = conn.getresponse()
result = response.read()

print(result.decode("utf-8"))